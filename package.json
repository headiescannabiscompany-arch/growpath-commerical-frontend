{
  "name": "growpath-ai-frontend",
  "version": "1.0.0",
  "description": "GrowPath AI React Native Frontend",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json}\"",
    "audit:ci": "node ./scripts/audit-ci.mjs",
    "test": "node --test --import ./tests/setup_fetch_spy.js tests/*.test.js tests/**/*.test.js",
    "test:live": "node scripts/test-live.js",
    "backend:start": "node scripts/start-backend.js",
    "acceptance": "node scripts/run-acceptance.js",
    "acceptance:live": "USE_LIVE_BACKEND=true ACCEPTANCE_COMMANDS='node --test --import ./tests/setup_fetch_spy.js tests/**/*.test.js' node scripts/run-acceptance.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "~1.18.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "expo": "~49.0.23",
    "expo-dev-client": "~2.4.13",
    "expo-device": "~5.4.0",
    "expo-document-picker": "~11.5.4",
    "expo-image-picker": "~14.3.2",
    "expo-notifications": "~0.20.1",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.2.2",
    "scheduler": "^0.23.2"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.0",
    "js-yaml": "^4.1.1",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0"
  },
  "overrides": {
    "ip": "2.0.1",
    "semver": "^7.6.3",
    "send": "^0.19.2",
    "webpack-dev-server": "^5.2.2",
    "@expo/config-plugins": "~7.2.2"
  }
}
