{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Backend: dev",
			"type": "shell",
			"command": "npm run dev",
			"options": {
				"cwd": "${workspaceFolder}/backend"
			},
			"isBackground": true,
			"problemMatcher": {
				"pattern": [
					{
						"regexp": ".",
						"file": 1,
						"location": 2,
						"message": 3
					}
				],
				"background": {
					"activeOnStart": true,
					"beginsPattern": "Server running",
					"endsPattern": "never"
				}
			}
		},
		{
			"label": "Frontend: expo",
